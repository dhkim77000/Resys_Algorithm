   "source": [
    "from collections import deque\n",
    "cards = deque(range(int(input())))\n",
    "while len(cards)>1:\n",
    "    cards.popleft()\n",
    "    cards.append(cards.popleft())\n",
    "print(cards[0]+1)"
   ]
